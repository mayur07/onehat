{
  "name": "angular2-full-stack",
  "version": "5.2.0",
  "license": "MIT",
  "author": "Davide Violante",
  "description": "Angular Full Stack project built using Angular 2+, Express, Mongoose and Node.",
  "angular-cli": {},
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"ng build --aot false -prod && tsc -p server && node dist/server/app.js\"",
    "postinstall": "tsc -p server && ng build --aot false -prod",
    "test": "ng test",
    "testbe": "tsc -p server && mocha dist/server/test --exit",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cli": "~1.7.3",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@auth0/angular-jwt": "~1.1.0",
    "@types/jasmine": "~2.8.3",
    "@types/jquery": "^3.3.1",
    "@types/node": "~6.0.60",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "core-js": "^2.4.1",
    "docxtemplater": "^3.6.1",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "html-docx-js": "^0.3.1",
    "html-pdf": "^2.2.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.2.1",
    "jszip": "^2.6.1",
    "jszip-utils": "0.0.2",
    "mongoose": "^5.0.13",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.4",
    "popper.js": "^1.14.3",
    "primeng": "^5.2.4",
    "quill": "^1.3.6",
    "rxjs": "^5.5.6",
    "tether": "1.4.0",
    "typescript": "~2.5.3",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/language-service": "^5.2.0",
    "@types/express": "^4.11.0",
    "@types/jasminewd2": "~2.0.2",
    "codelyzer": "~4.0.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.3",
